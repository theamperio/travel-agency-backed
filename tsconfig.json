{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts", "index.ts", "controllers/**/*.ts", "models/**/*.ts", "routes/**/*.ts"],
  "exclude": ["node_modules"]
}